<Application
    x:Class="MyGuide.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone"
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
    xmlns:local="clr-namespace:MyGuide"
    xmlns:converters="clr-namespace:MyGuide.Converters">

    <!--Application Resources-->

    <Application.Resources>
        <local:Bootstrapper x:Key="Bootstrapper" />
        <local:LocalizedStrings xmlns:local="clr-namespace:MyGuide" x:Key="LocalizedStrings" />
        <converters:BooleanToVisibilityConverter xmlns:converters="clr-namespace:MyGuide.Converters" x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
</Application>